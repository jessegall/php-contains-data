{
    "name": "jessegall/has-array-data",
    "description": "A package that provides a trait to get and set items in an array using dot notation",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JesseGall\\HasArrayData\\": "src/"
        }
    },
    "autoload-dev": {
        "JesseGall\\HasArrayData\\Tests": "tests/"
    },
    "authors": [
        {
            "name": "Jesse Gall",
            "email": "jesse-gall@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
